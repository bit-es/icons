{
    "name": "bit-es/icons",
    "description": "This is bites package with icons mainly used in Bites ecosystem, that maybe used by any other views",
    "keywords": [
        "bit-es",
        "bases",
        "laravel",
        "filament"
    ],
    "homepage": "https://github.com/bit-es/icons",
    "support": {
        "issues": "https://github.com/bit-es/icons/issues",
        "source": "https://github.com/bit-es/icons"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bit-es",
            "email": "bit.ecosystem@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Bites\\Icons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bites\\Icons\\BitesIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}